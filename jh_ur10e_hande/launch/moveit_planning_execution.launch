<launch>
  <arg name="sim" default="true"/>
  <arg name="debug" default="false"/>

  <param name="robot_description"
         command="$(find xacro)/xacro '$(find jh_ur10e_hande)/urdf/ur10e_hande.urdf.xacro'" />
  <param name="robot_description_semantic"
         textfile="$(find jh_ur10e_hande)/config/jh_ur10e_hande.srdf" />
  <rosparam file="$(find jh_ur10e_hande)/config/fake_controllers.yaml" command="load" />

  <include file="$(find ur10e_moveit_config)/launch/move_group.launch">
    <arg name="debug" value="$(arg debug)" />
  </include>
</launch>
